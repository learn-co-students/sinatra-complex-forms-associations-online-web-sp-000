<h1>Welcome</h1>
<h2>Create a new owner</h2>

<form action="/owners" method="POST">
<input type="text" id="owner_name" name="owner[name]">
<input type="submit" value="submit">
</form>


<h1>Existing owners</h1>
<%@owners.each do |owner|%>
<lable><%owner.name%></lable>
<input type="checkbox" name="owner[pet_ids][]" id="owner_<%=owner.id%>" value="<%=owner.id%>">